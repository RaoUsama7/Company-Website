RewriteEngine On
RewriteCond %{THE_REQUEST} /([^.]+)\.html [NC]
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^projects$ /projects.html [L]
RewriteRule ^about$ /about.html [L]
RewriteRule ^services$ /services.html [L]
RewriteRule ^index$ /index.html [L]
RewriteRule ^services$ /services.html [L]
